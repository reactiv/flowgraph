      "displayName": "Member Of",
      "from": "Author",
      "to": "Channel",
      "direction": "out"
    }
  ],
  "rules": [
    {
      "id": "message-must-have-channel",
      "when": {
        "nodeType": "Message",
        "transitionTo": "active"
      },
      "requireEdges": [
        {
          "edgeType": "POSTED_IN",
          "minCount": 1
        }
      ],
      "message": "A message must be posted in a channel"
    },
    {
      "id": "message-must-have-author",
      "when": {
        "nodeType": "Message",
        "transitionTo": "active"
      },
      "requireEdges": [
        {
          "edgeType": "AUTHORED_BY",
          "minCount": 1
        }
      ],
      "message": "A message must have an author"
    },
    {
      "id": "thread-reply-must-belong-to-thread",
      "when": {
        "nodeType": "Message",
        "transitionTo": "active"
      },
      "requireEdges": [
        {
          "edgeType": "PART_OF_THREAD",
          "minCount": 0
        }
      ],
      "message": "Thread replies must be linked to their parent thread"
    }
  ],
